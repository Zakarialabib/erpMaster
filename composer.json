{
    "name": "zakarialabib/erpmaster",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "automattic/woocommerce": "^3.1",
        "calebporzio/sushi": "^2.4",
        "carlos-meneses/laravel-mpdf": "^2.1",
        "guzzlehttp/guzzle": "^7.2",
        "jantinnerezo/livewire-alert": "^3.0",
        "jeroendesloovere/vcard": "^1.7",
        "laravel/framework": "^11.0",
        "laravel/socialite": "^5.9",
        "laravel/tinker": "^2.8",
        "livewire/livewire": "^3.0.1",
        "maatwebsite/excel": "^3.1.55",
        "milon/barcode": "^11.0.0",
        "shopify/shopify-api": "^5.1",
        "socialiteproviders/facebook": "*",
        "socialiteproviders/google": "^4.1",
        "spatie/laravel-backup": "^8.1",
        "spatie/laravel-medialibrary": "^11.4.5",
        "spatie/laravel-permission": "^6.4.0",
        "spatie/laravel-sitemap": "^7.2.0",
        "zakarialabib/flysystem-google-drive-ext": "^0.0.1",
        "zakarialabib/shoppingcart": "^1.2.0",
        "dcblogdev/laravel-db-sync": "^1.1",
        "google/apiclient": "^2.15",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "smalot/pdfparser": "*",
        "laravel/passport": "^12.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.7",
        "fakerphp/faker": "^1.9.1",
        "kkomelin/laravel-translatable-string-exporter": "^1.17",
        "laravel/breeze": "^2.0",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^8.1",
        "nunomaduro/larastan": "^2.0",
        "nunomaduro/phpinsights": "^2.7",
        "phpunit/phpunit": "^10.1",
        "rector/rector": "^0.18.2",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "Google\\Task\\Composer::cleanup",
            "@php artisan optimize:clear"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan optimize:clear",
            "@php artisan migrate --force"    
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "pre-autoload-dump": "Google\\Task\\Composer::cleanup",
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php artisan migrate --graceful --ansi",
            "@php artisan db:seed --force"    
        ]
    },
    "extra": {
        "extra": {
            "google/apiclient-services": [
                "Analytics",
                "SearchConsole",
                "Gmail",
                "Drive",
                "Sheets",
                "PeopleService",
                "ShoppingContent",
                "SiteVerification"
            ],
            "laravel": {
                "dont-discover": []
            }
        },
        "config": {
            "optimize-autoloader": true,
            "preferred-install": "dist",
            "sort-packages": true,
            "allow-plugins": {
                "pestphp/pest-plugin": true,
                "php-http/discovery": true,
                "dealerdirect/phpcodesniffer-composer-installer": true
            }
        },
        "minimum-stability": "stable",
        "prefer-stable": true
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": false,
            "dealerdirect/phpcodesniffer-composer-installer": false
        }
    }
}
